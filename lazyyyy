
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("s Hub", "DarkTheme")
local Tab = Window:NewTab("Main")						
local Section = Tab:NewSection("Auto Raid")

Section:NewToggle("Q Monster", " ", function(a)
AutoEquiped = a
end)

spawn(function()
while wait() do
if AutoEquiped then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Monster" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Troll Monster", " ", function(c)
AutoEquipe = c
end)

spawn(function()
while wait() do
if AutoEquipe then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Troll Monster" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Tanky Monster", " ", function(h)
Aud = h
end)

spawn(function()
while wait() do
if Aud then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Tanky Monster" then
		v.Humanoid.Health  = die
	end
end
end)
end
end
end)

Section:NewToggle("Q Ice Master", " ", function(o)
AutoEqu = o
end)

spawn(function()
while wait() do
if AutoEqu then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Ice Master" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Light Master", " ", function(t)
AutoEquipde = t
end)

spawn(function()
while wait() do
if AutoEquipde then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Light Master" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Quake Master", " ", function(b)
AutoEquipea = b
end)

spawn(function()
while wait() do
if AutoEquipea then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Quake Master" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

local Tab = Window:NewTab("key bind")						
local Section = Tab:NewSection(" ")

Section:NewKeybind("Keybind", "Keybind", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)

if game.PlaceId == 11189305210 then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").GateModel.Gate.CFrame
end

if game.PlaceId == 12264299372 then
	while wait() do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-217.152802, 3560.37085, -5545.27637, 0.599122226, -1.09726246e-08, -0.80065757, 9.96048684e-08, 1, 6.08285831e-08, 0.80065757, -1.16193156e-07, 0.599122226)
	end
end


