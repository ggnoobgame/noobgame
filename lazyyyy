
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("s Hub", "DarkTheme")
local Tab = Window:NewTab("Main")						
local Section = Tab:NewSection("Auto Raid")

Section:NewToggle("Q Monster", " ", function(a)
AutoEquiped = a
end)

spawn(function()
while wait() do
if AutoEquiped then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Monster" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Troll Monster", " ", function(c)
AutoEquipe = c
end)

spawn(function()
while wait() do
if AutoEquipe then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Troll Monster" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Ice Master", " ", function(o)
AutoEqu = o
end)

spawn(function()
while wait() do
if AutoEqu then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Ice Master" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Light Master", " ", function(t)
AutoEquipde = t
end)

spawn(function()
while wait() do
if AutoEquipde then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Light Master" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

Section:NewToggle("Q Quake Master", " ", function(b)
AutoEquipea = b
end)

spawn(function()
while wait() do
if AutoEquipea then
pcall(function()
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
	if v.name == "Quake Master" then
		v.Humanoid.Health  = d
	end
end
end)
end
end
end)

local Tab = Window:NewTab("key bind")						
local Section = Tab:NewSection(" ")

Section:NewKeybind("Keybind", "Keybind", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)

if game.PlaceId == 11189305210 then
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9829.77344, 1.04352295, 290.295685, 0.917115867, -0.33532992, -0.215527683, 0.318267673, 0.941528738, -0.110586219, 0.240008369, 0.0328248739, 0.970215678)
end
